<script>

// Vanilla JavaScript
const categories = document.getElementById('categories');
const categoryItems = categories.getElementsByTagName('li');

for (let i = 0; i < categoryItems.length; i++) {
  categoryItems[i].addEventListener('click', function () {
    // Remove 'selected' class from previously selected category
    const selectedCategory = categories.querySelector('.selected');
    if (selectedCategory) {
      selectedCategory.classList.remove('selected');
    }

    // Add 'selected' class to the clicked category
    this.classList.add('selected');
  });
}


// jQuery
$(document).ready(function() {
  $('#categories li').click(function() {
    // Remove 'selected' class from previously selected category
    $('#categories li.selected').removeClass('selected');

    // Add 'selected' class to the clicked category
    $(this).addClass('selected');
  });
});


	
	const val = document.getElementById('kai').value
