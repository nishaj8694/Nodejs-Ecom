<script>
    function show(id){
        $.ajax({
            url:"/addTocart",
            method:"post",
            data:{
                id:id
            },
            success:(response)=>{
                if(response.success){
                    swal.fire({
                        position:'center',
                        icon:'success',
                        title:'product  added to cart',
                        showConfirmButton:false,
                        timer:1500,
                       

